  "onload": [
    "var x, y, width, height;",
    "",
    "width = RUR.randint(3, 4) * 3;",
    "height = 6;",
    "RUR.set_world_size(width, height);",
    "RUR.fill_background(\"white\");",
    "",
    "function add_sea() {",
    "    for(let x=1; x<=width; x++){",
    "        for(let y=1; y<=4; y++){",
    "            RUR.add_background_tile(\"water\",x,y);",
    "        }",
    "    }",
    "}",
    "                                    ",
    "function make_dive(){",
    "    let dives = 0;",
    "    for(let x = 1; x<width; x+=3){",
    "       dives++;",
    "       let depth = RUR.randint(1, 3);",
    "       for(let y = 4; y >= 4-depth; y--){",
    "           RUR.add_background_tile(\"ice\",x,y); ",
    "       }",
    "       RUR.add_background_tile(\"ice\",x+1,4-depth);",
    "       RUR.add_object(\"star\", x, 4-depth);",
    "       RUR.add_object(\"star\", x+1, 4-depth);",
    "    }",
    "    RUR.add_background_tile(\"mud\",width-1,5);",
    "    RUR.add_background_tile(\"mud\",width,5);",
    "    RUR.add_wall(\"west\",width-1,5);",
    "    RUR.add_wall(\"north\",width-1,5);",
    "    RUR.add_wall(\"north\",width,5);",
    "    RUR.add_final_position(\"house\",width,6);",
    "    RUR.add_object(\"star\", width-1,6, {\"number\": dives*2, \"goal\":\"true\"});",
    "}",
    "",
    "// bottom row",
    "add_sea();",
    "make_dive();",
    "            ",
    "          "
